<% provide(:title, 'Add Warframe') %>

<h1 class = "white">Add Warframe</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@warframe) do |w| %>

      <%= w.label :name %>
      <%= w.text_field :name, class: 'form-control' %>

      <%= w.label :image_url, "Image Url" %>
      <%= w.text_field :image_url, class: 'form-control' %>

      <%= w.label :whereToGet , "Where To Get"%>
      <%= w.text_area :whereToGet, class: 'form-control' %>


      <%= w.submit "Add Warframe", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
