        <!-- <h1>Where To Get: <%= @warframe.whereToGet %> </h1> -->

        <div id="warframe-view-wrapper_">
            <div id="warframe-view-stats-div_">
                <h4 class="warframe-view-stats_">Mastery Rank</h4>
                <h4 class="warframe-view-stats_">Health</h4>
                <h4 class="warframe-view-stats_">Shield</h4>
                <h4 class="warframe-view-stats_">Armor</h4>
                <h4 class="warframe-view-stats_">Energy</h4>
            </div>
        </div>
    </div>
</div>
