<div id ="weapon_data">
    <h1 id="weapon-view-header_"> <%= @weapon.name %> </h1>
    <div id="image-and-text">
        <img id="weapon-view-image_" src=<%= @weapon.image_url %>  alt=<%= @weapon.name %> title=<%= @weapon.name %> >
        <!-- <h1>Where To Get: <%= @weapon.whereToGet %> </h1> -->

        <div id="weapon-view-wrapper_">
            <div id="weapon-view-stats-div_">
                <h4 class="weapon-view-stats_">Mastery Rank</h4>
                <h4 class="weapon-view-stats_">Health</h4>
                <h4 class="weapon-view-stats_">Shield</h4>
                <h4 class="weapon-view-stats_">Armor</h4>
                <h4 class="weapon-view-stats_">Energy</h4>
            </div>
        </div>
    </div>
</div>
